{
	"name": "nextras/composer-autoload-simplifier",
	"type": "composer-plugin",
	"description": "Simplifies autoloader generated by Composer",
	"keywords": ["composer", "autoloader"],
	"homepage": "https://github.com/nextras/composer-autoload-simplifier",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/nextras/composer-autoload-simplifier/issues"
	},
	"require": {
		"php": ">=7.0",
		"composer-plugin-api": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Nextras\\ComposerAutoloadSimplifier\\": "src/"
		}
	},
	"extra": {
		"class": "Nextras\\ComposerAutoloadSimplifier\\Plugin"
	}
}
